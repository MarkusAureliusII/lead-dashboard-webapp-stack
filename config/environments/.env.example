# Production Environment Configuration Template
# Copy this file to .env.production and fill in your actual values

# Application Settings
NODE_ENV=production
APP_NAME="Lead Persona Dashboard"
APP_VERSION=1.0.0
SERVER_IP=your_server_ip_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=http://your_server_ip:8000
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Database Configuration
POSTGRES_PASSWORD=your_postgres_password_here
JWT_SECRET=your_jwt_secret_here
ANON_KEY=your_anon_key_here
SERVICE_ROLE_KEY=your_service_role_key_here

# Studio Configuration
STUDIO_DEFAULT_ORGANIZATION="WebApp Backend"
STUDIO_DEFAULT_PROJECT="Production"
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=your_secure_password_here

# API Configuration
API_EXTERNAL_URL=http://your_server_ip:8000
SITE_URL=http://your_server_ip
SUPABASE_PUBLIC_URL=http://your_server_ip:8000

# Security
SECRET_KEY_BASE=your_secret_key_base_here
VAULT_ENC_KEY=your_vault_encryption_key_here

# Logging
LOG_LEVEL=info
ENABLE_LOGGING=true

# Performance
MAX_CONNECTIONS=100
TIMEOUT=30000

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# N8n Webhook Configuration
N8N_WEBHOOK_URL=your_n8n_webhook_url_here
N8N_API_KEY=your_n8n_api_key_here